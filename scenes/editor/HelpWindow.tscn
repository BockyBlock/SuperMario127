script/source = "extends RichTextLabel

# This dictionary tells the script the position to scroll to on click.
# Key = URL ref (string), Value = scroll distance (int)
var scroll_values = {
	\"warps\" : 331,
	\"music\" : 926,
	\"palettes\" : 1325,
	\"boo\" : 1547,
	\"special\" : 1770,
}

# Triggered on clicking the URL, scrolls to respective section.
func _on_meta_clicked(meta):
	$\"..\".scroll_vertical = scroll_values[meta]
"

