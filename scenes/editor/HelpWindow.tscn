[sub_resource type="GDScript" id=12]
script/source = "extends ScrollContainer

onready var faq_container = get_node(\"../FAQ\")

onready var hotkeys_button = get_node(\"../HotkeysButton\")
onready var faq_button = get_node(\"../FAQButton\")

func _ready():
	var _connect = hotkeys_button.connect(\"pressed\", self, \"switch_to_hotkeys\")
	_connect = faq_button.connect(\"pressed\", self, \"switch_to_faq\")

func switch_to_hotkeys():
	faq_container.visible = false
	faq_button.disabled = false
	
	hotkeys_button.disabled = true
	visible = true

func switch_to_faq():
	faq_container.visible = true
	faq_button.disabled = true
	
	hotkeys_button.disabled = false
	visible = false
"

